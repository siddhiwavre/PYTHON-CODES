m=input("enter a number")
m=int(m)
def sumofsquares(m):
  i=1
  while i * i <= m :
    j = 1
    while(j * j <= m) :
      if (i * i + j * j == m) :
        print (i,"^2","+",j,"^2","=",m)
        return True
        return 0
      j = j + 1
    i = i + 1
  if (i * i + j * j != m) :
         print ("Not a sum of square")
sumofsquares(m)
